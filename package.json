{
  "name": "backend_port",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:auth": "playwright test tests/e2e/auth.spec.js",
    "test:api": "playwright test tests/e2e/api.spec.js",
    "test:report": "playwright show-report",
    "analyze": "cross-env ANALYZE=true npm run build",
    "seed:crm": "node scripts/production/seedSystemSettings.js",
    "seed:universities": "node scripts/production/seedUniversities.js",
    "seed:all-universities": "node scripts/production/seedAllUniversitiesNew.js",
    "seed:153-universities": "node scripts/production/seed153Universities.js",
    "db:indexes": "node scripts/production/createIndexes.js",
    "seed:all": "npm run seed:crm && npm run seed:all-universities && npm run db:indexes",
    "update:universities": "node scripts/maintenance/updateUniversitiesComplete.js",
    "update:153universities": "node scripts/maintenance/updateTo153Universities.js",
    "update:destinations": "node scripts/maintenance/updateStudyDestinations.js",
    "update:times": "node scripts/maintenance/updateStudyTimes.js",
    "update:sources": "node scripts/maintenance/updateSources.js",
    "verify:destinations": "node scripts/maintenance/verifyStudyDestinations.js",
    "parse:info": "node scripts/maintenance/parseInfoFile.js",
    "predeploy": "npm run lint && npm run build",
    "postinstall": "echo 'Installation complete. Run npm run check:env to verify environment setup.'"
  },
  "dependencies": {
    "@auth/mongodb-adapter": "^3.7.4",
    "@cloudinary/url-gen": "^1.19.0",
    "@sentry/nextjs": "^7.99.0",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.4.2",
    "cloudinary": "^2.2.0",
    "dompurify": "^3.3.1",
    "dotenv": "^17.2.3",
    "ioredis": "^5.8.2",
    "isomorphic-dompurify": "^2.34.0",
    "mongodb": "^6.5.0",
    "mongoose": "^8.3.0",
    "multiparty": "^4.2.3",
    "next": "^16.0.7",
    "next-auth": "^4.24.11",
    "playwright": "^1.57.0",
    "react": "^18",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.0.1",
    "react-markdown": "^9.0.1",
    "react-sortablejs": "^6.1.4",
    "sortablejs": "^1.15.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-react": "^5.11.0",
    "swr": "^2.3.3",
    "use-debounce": "^10.0.4",
    "validator": "^13.15.23",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.0.7",
    "@playwright/test": "^1.40.0",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.4.1"
  }
}
